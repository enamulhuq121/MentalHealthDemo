           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoMStatus1" android:text="বর্তমানে বিবাহিত/তা" android:paddingLeft="5dp"/>
           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoMStatus2" android:text="বিচ্ছিন্না/ তালাকপ্রাপ্তা/পরিত্যক্তা" android:paddingLeft="5dp"/>
           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoMStatus3" android:text="বিধবা/বিপত্নীক" android:paddingLeft="5dp"/>
           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoMStatus4" android:text="কখনো বিয়ে হয়নি" android:paddingLeft="5dp"/>
