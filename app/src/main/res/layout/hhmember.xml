           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoSchoolLevel4" android:text="প্রাকপ্রাথমিক/ শিশু শ্রেণী" android:paddingLeft="5dp"/>
