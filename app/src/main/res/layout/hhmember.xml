        <TextView android:id="@+id/VlblHHID" android:text="খানা" android:layout_width="250sp" android:layout_height="wrap_content"  android:layout_marginBottom="10sp"/>
        <EditText android:id="@+id/txtHHID" android:maxLength="50" android:layout_width="match_parent" android:inputType="text" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp"/>
    </LinearLayout>
    <View android:id="@+id/lineHHID" android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
    <LinearLayout android:id="@+id/secMSlNo" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView android:id="@+id/lblMSlNo" android:text="1." android:layout_width="40sp" android:layout_height="wrap_content" />
        <TextView android:id="@+id/VlblMSlNo" android:text="খানা  লাইন নম্বর (বাড়ি ভিত্তিক প্রত্যেক খানার জন্য আলাদা আলাদা লাইন হবে)" android:layout_width="250sp" android:layout_height="wrap_content"  android:layout_marginBottom="10sp"/>
         <EditText android:id="@+id/txtMSlNo" android:maxLength="2" android:layout_width="60sp" android:inputType="number" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" />
    </LinearLayout>
    <View android:id="@+id/lineMSlNo" android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
    <LinearLayout android:id="@+id/seclbl7" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowHeading">
        <TextView android:id="@+id/Vlbllbl7" android:text="" android:visibility="gone" android:layout_width="40sp" android:layout_height="wrap_content" android:textColor="@color/form_heading_font_color" />
        <TextView android:id="@+id/lblHlbl7" android:text="সাধারণতঃ বসবাসকারী সদস্য এবং অতিথিবৃন্দ ,আপনার খানায় সাধারণতঃ যারা বসবাস করেন এবং যে সমস্ত অতিথি গতরাতে আপনার খানায় ঘুমিয়েছেন, দয়া করে তাদের নাম বলুন। খানা প্রধান থেকে শুরু করুন।প্রত্যেক ব্যক্তির নাম তালিকাবদ্ধ এবং তাদের সম্পর্ক ও তারা পুরুষ না মহিলা তা লিপিবদ্ধ করার পরে তালিকা সম্পূর্ণ হয়েছে কিনা তা নিশ্চিত হবার জন্যে ছ২অ—২ঈ জিজ্ঞাসা করুন।অতঃপর প্রত্যেক ব্যক্তির জন্যে ৮—১৯ কলামগুলোতে অবস্থিত প্রাসঙ্গিক/সংশ্লিষ্ট প্রশ্নগুলো জিজ্ঞাসা করুন।" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="@color/form_heading_font_color"  />
    </LinearLayout>
    <View android:id="@+id/linelbl7" android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
    <LinearLayout android:id="@+id/secName" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">
        <TextView android:id="@+id/lblName" android:text="7." android:layout_width="40sp" android:layout_height="wrap_content" />
        <TextView android:id="@+id/VlblName" android:text="ব্যক্তির নাম" android:layout_width="250sp" android:layout_height="wrap_content"  android:layout_marginBottom="10sp"/>
