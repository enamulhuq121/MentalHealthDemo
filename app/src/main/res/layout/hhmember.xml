        <TextView android:id="@+id/VlblStayLastNight" android:text="(নাম) কি গত রাতে এই খানায় ঘুমিয়ে-ছিলেন?
" android:layout_width="250sp" android:layout_height="wrap_content"  android:layout_marginBottom="10sp"/>
        <RadioGroup android:id="@+id/rdogrpStayLastNight" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">         
           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoStayLastNight1" android:text="হ্যাঁ " android:paddingLeft="5dp"/>
           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoStayLastNight2" android:text="না" android:paddingLeft="5dp"/>
