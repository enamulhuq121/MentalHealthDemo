           <RadioButton android:layout_height="wrap_content" android:layout_width="wrap_content" android:paddingRight="10dp" android:id="@+id/rdoSchoolLevel2" android:text="মাধ্যমিক বিদ্যালয় " android:paddingLeft="5dp"/>
